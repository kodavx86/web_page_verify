 <script type="text/javascript">
  function set_default() {
  	d_obj = document.getElementById("search_b");
  	if(0 == d_obj.value.length) {
  		d_obj.style.color = "grey";
  		d_obj.value = "Enter code you want to search for here";
  	}
  }
  function un_set_default() {
  	d_obj = document.getElementById("search_b");
  	d_obj.style.color = "black";
  	if(d_obj.value.toLowerCase() === "Enter code you want to search for here".toLowerCase()) {
  		d_obj.value = "";
  	}
  }
 </script>
